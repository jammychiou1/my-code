int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t = 0;
    while (t++ < 1000000) {
        system("cls");
        for (int i = 0; i < 12; i++) {
            for (int j = 0; j < 105; j++) {
                if ((t+j) % 150 >= 105) cout << ' ';
                else cout << (table[i][(t+j) % 150] ? '#' : ' ');
            }
            cout << endl;
        }
        Sleep(33);
    }
}
